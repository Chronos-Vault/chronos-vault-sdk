{
  "name": "@chronos-vault/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for integrating Chronos Vault's revolutionary multi-chain security platform into your applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "blockchain",
    "defi",
    "security",
    "multi-chain",
    "ethereum",
    "solana", 
    "ton",
    "zero-knowledge",
    "quantum-resistant",
    "trinity-protocol",
    "sdk",
    "typescript",
    "chronos-vault"
  ],
  "author": "Chronos Vault Team",
  "license": "MIT",
  "homepage": "https://github.com/Chronos-Vault/chronos-vault-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chronos-Vault/chronos-vault-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Chronos-Vault/chronos-vault-sdk/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@tonconnect/sdk": "^3.0.0",
    "ethers": "^6.9.2",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/crypto-js": "^4.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}